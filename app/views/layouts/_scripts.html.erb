<script>
    function phoneNumCheck(e) {
        var input = e.key
        var regex = /([^\d+])/
        if (regex.test(input) && e.which != 8) {
            e.preventDefault()
        }
    };

    $(document).ready(function(){
        // Dropdown menu
        $('select').formSelect();

        // Date picker.
        var thisYear = new Date().getFullYear()
        $('.datepicker').datepicker({
            yearRange: [thisYear - 50, thisYear]
        });

        $('.tooltipped').tooltip({
            enterDelay: 700,
        });

        $('.dropdown-trigger').dropdown();
    });

</script>